{
  "name": "server",
  "version": "1.0.0",
  "main": "build/server.js",
  "license": "MIT",
  "scripts": {
    "build": "rimraf build && tsc && tsc-alias",
    "test": "tsc && tsc-alias && node build/__test__/test.js",
    "dev": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"npx tsc-alias -w\" \"nodemon -q build/server.js\"",
    "start": "cross-env NODE_ENV=production node build/server.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^6.0.1",
    "http-status-codes": "^2.2.0",
    "morgan": "^1.10.0",
    "socket.io": "^4.5.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.18",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "ts-alias": "^0.0.6",
    "typescript": "^4.9.4"
  }
}
